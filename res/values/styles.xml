<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <!-- Customized Buttons Style -->

    <style name="ButtonText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">@dimen/buttonTextSize</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
    </style>

    <style name="NumPadText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">@dimen/buttonTextSize</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
    </style>

    <style name="NumPadText2">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:textSize">@dimen/buttonTextSize</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
    </style>

    <style name="ExpanderDataText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">@dimen/textSize28</item>
        <item name="android:textStyle">normal</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">0</item>
        <item name="android:shadowRadius">0</item>
    </style>

    <style name="Expander2DataText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">@dimen/textSize28</item>
        <item name="android:textStyle">normal</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">0</item>
        <item name="android:shadowRadius">0</item>
    </style>

    <!-- Title text style -->
    <style name="TitleText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#4697f2</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">@dimen/textSize30</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
    </style>

    <!-- Content text style -->
    <style name="ContentText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:gravity">fill_horizontal</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">@dimen/textContentDefaultFontSize</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <!-- App type text style (Lite, Full) -->
    <style name="AppTypeText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#FA6A39</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">@dimen/textSize28</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
    </style>

    <style name="AppSloganText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#4697f2</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">@dimen/textSize12</item>
        <item name="android:textStyle">normal</item>
        <!--
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        -->
    </style>
    <!-- Result page text1 -->
    <style name="PerformanceText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#4697f2</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">@dimen/textSize55</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
    </style>

    <style name="InfoOnResultsPageText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#4697f2</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">@dimen/textSize24</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
    </style>

    <color name="white_opaque">#FFFFFFFF</color>
    <color name="pitch_black">#FF000000</color>
    <color name="ve_expander1_title_bg_color">#323331</color>
    <color name="black">#000000</color>
    <color name="dark_gray">#757475</color>

    <style name="VisionExpandTheme" parent="android:Theme">

        <!--
        <item name="android:windowTitleSize">@dimen/windowTitleSize</item>
        <item name="android:windowTitleBackgroundStyle">@style/Expander1WindowTitleBackground</item>
        -->
        <!-- <item name="android:windowBackground">@style/Expander1WindowBackground</item> -->
        <!-- <item name="android:background">@color/white_opaque</item> -->
        <item name="android:windowBackground">@color/white_opaque</item>
        <!-- <item name="android:colorBackground">@color/white_opaque</item> -->
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textAppearance">@color/black</item>
    </style>

    <!-- Settings specific theme -->

    <style name="ListSeparator">
        <item name="android:background">@android:color/darker_gray</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">3sp</item>
    </style>

    <color name="custom_theme_color">#e6e6e6</color>

    <!--
    <style name="Preference.Category">
        <item name="android:layout">@android:layout/preference_category</item>
        <item name="android:shouldDisableView">false</item>
        <item name="android:selectable">false</item>
    </style>

    <style name="PreferenceListHeader" parent="@android:attr/listSeparatorTextViewStyle">
        <item name="android:textColor">#000000</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">#cccccc</item>
        <item name="android:paddingTop">6px</item>
        <item name="android:paddingBottom">6px</item>
        <item name="android:paddingLeft">12px</item>
    </style>
    -->

    <style name="VisionExpandSettingsTheme" parent="android:Theme">
        <item name="android:windowTitleSize">@dimen/windowTitleSize</item>
        <item name="android:windowTitleBackgroundStyle">@style/Expander1WindowTitleBackground</item>
        <!-- <item name="android:windowBackground">@style/Expander1WindowBackground</item> -->
        <!-- <item name="android:background">@color/white_opaque</item> -->
        <item name="android:windowBackground">@android:color/white</item>
        <!-- <item name="android:colorBackground">@color/white_opaque</item> -->
        <item name="android:textColor">@color/black</item>
        <item name="android:textAppearance">@color/black</item>
        <!-- <item name="android:listSeparatorTextViewStyle">@style/PreferenceListHeader</item> -->
        <item name="android:listSeparatorTextViewStyle">@style/ListSeparator</item>
        <item name="android:textColorSecondary">@android:color/black</item>
    </style>

    <!-- Settings theme end -->

    <style name="Expander1WindowTitleBackground">

        <!-- <item name="android:background">@android:color/transparent</item> -->
        <item name="android:background">@color/ve_expander1_title_bg_color</item>
        <item name="android:padding">0px</item>
    </style>

    <!--
      <item name="android:background">@color/white_opaque</item>
      <item name="android:windowBackground">@color/white_opaque</item>
      <item name="android:colorBackground">@color/white_opaque</item>
    -->

    <style name="Expander1WindowBackground">
        <item name="android:background">@android:color/white</item>
        <item name="android:padding">0px</item>
    </style>

</resources>